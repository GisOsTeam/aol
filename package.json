{
  "name": "@gisosteam/aol",
  "version": "0.1.7",
  "description": "OpenLayers sources extensions",
  "scripts": {
    "test": "jest",
    "jest": "jest",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'",
    "lint": "npm run prettier && eslint . --ext .ts,.tsx",
    "genindex": "barrelsby -d ./src -D -L -l all -q -e sample -e __tests__ -e .d.ts",
    "build": "rimraf lib && npm run genindex && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "prepublishOnly": "[[ \"$PWD\" =~ /lib ]]",
    "pub": "pushd ./ && npm run build && cd lib && npm publish --access public && popd"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/aol.git"
  },
  "keywords": [
    "Web mapping",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@types/jest": "^26.0.20",
    "@types/jszip": "^3.4.1",
    "@types/ol": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "barrelsby": "^2.2.0",
    "eslint": "^7.21.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "ol": "^6.4.3",
    "prettier": "^2.2.1",
    "proj4": "2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.2",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@turf/turf": "^6.3.0",
    "bhreq": "^0.1.1",
    "jszip": "^3.6.0",
    "ol-mapbox-style": "^6.3.1",
    "shapefile2geojson": "^1.0.7"
  },
  "peerDependencies": {
    "ol": "^6.4.3",
    "proj4": "^2.7.0"
  },
  "engines": {
    "node": ">= 10.10.0",
    "npm": ">= 6.4.1"
  }
}
