{
  "name": "@gisosteam/aol",
  "version": "0.4.3",
  "description": "OpenLayers sources extensions",
  "scripts": {
    "test": "jest",
    "jest": "jest",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'",
    "lint": "npm run prettier && eslint src/** --ext .ts,.tsx",
    "genindex": "barrelsby -d ./src -D -L -l all -q -e sample -e __tests__ -e .d.ts",
    "build": "rimraf lib && npm run genindex && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "prepublishOnly": "bash -c '[[ \"$PWD\" =~ /lib ]]'",
    "pub": "bash -c 'pushd ./ && npm run build && cd lib && npm publish --access public && popd'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/aol.git"
  },
  "keywords": [
    "Web mapping",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@types/jest": "^29.5.12",
    "@types/jszip": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "babel-jest": "^29.7.0",
    "barrelsby": "^2.8.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "ol": "^9.1.0",
    "prettier": "^3.2.5",
    "proj4": "^2.11.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "5.4.3"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "bhreq": "^0.1.2",
    "jszip": "^3.10.1",
    "shapefile2geojson": "^1.0.7",
    "simplepolygon": "^1.2.3"
  },
  "peerDependencies": {
    "ol": "^7.3.0",
    "proj4": "^2.8.1"
  },
  "engines": {
    "node": ">= 18.20.1",
    "npm": ">= 10.5.0"
  }
}
