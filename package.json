{
  "name": "@gisosteam/aol",
  "version": "0.0.24",
  "description": "OpenLayers sources extentions",
  "scripts": {
    "test": "jest",
    "jest": "jest",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'",
    "lint": "npm run prettier && tslint -c tslint.json -p tsconfig.json -s '{src, test}/**/*.{ts}'",
    "build": "rimraf lib && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "prepublishOnly": "[[ \"$PWD\" =~ /lib ]]",
    "pub": "pushd ./ && npm run build && cd lib && npm publish --access public && popd"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/aol.git"
  },
  "keywords": [
    "Web mapping",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/jest": "^25.2.1",
    "@types/jszip": "^3.1.7",
    "@types/ol": "^6.3.1",
    "babel-jest": "^25.5.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-canvas-mock": "^2.2.0",
    "ol": "6.3.1",
    "prettier": "^2.0.5",
    "proj4": "2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "bhreq": "0.0.12",
    "jszip": "^3.4.0",
    "ol-mapbox-style": "^6.1.1",
    "shapefile2geojson": "^1.0.7"
  },
  "peerDependencies": {
    "ol": "6.3.1",
    "proj4": "2.6.1"
  },
  "engines": {
    "node": ">= 10.10.0",
    "npm": ">= 6.4.1"
  }
}
