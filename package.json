{
  "name": "@gisosteam/aol",
  "version": "0.2.5-SNAPSHOT-1",
  "description": "OpenLayers sources extensions",
  "scripts": {
    "test": "jest",
    "jest": "jest",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'",
    "lint": "npm run prettier && eslint . --ext .ts,.tsx",
    "genindex": "barrelsby -d ./src -D -L -l all -q -e sample -e __tests__ -e .d.ts",
    "build": "rimraf lib && npm run genindex && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "prepublishOnly": "[[ \"$PWD\" =~ /lib ]]",
    "pub": "pushd ./ && npm run build && cd lib && npm publish --access public && popd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/aol.git"
  },
  "keywords": [
    "Web mapping",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@types/jest": "^26.0.23",
    "@types/jszip": "^3.4.1",
    "@types/ol": "6.5.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.5",
    "barrelsby": "^2.2.0",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-canvas-mock": "^2.3.1",
    "ol": "^6.5.0",
    "prettier": "^2.3.2",
    "proj4": "2.7.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "4.3.4"
  },
  "dependencies": {
    "@turf/turf": "^6.4.0",
    "bhreq": "^0.1.2",
    "jszip": "^3.6.0",
    "ol-mapbox-style": "^6.3.2",
    "shapefile2geojson": "^1.0.7",
    "simplepolygon": "1.2.3"
  },
  "peerDependencies": {
    "@types/ol": "6.5.1",
    "ol": "^6.5.0",
    "proj4": "^2.7.4"
  },
  "engines": {
    "node": ">= 10.10.0",
    "npm": ">= 6.4.1"
  }
}
