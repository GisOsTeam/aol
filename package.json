{
  "name": "@gisosteam/aol",
  "version": "0.0.9",
  "description": "OpenLayers sources extentions",
  "main": "index.js",
  "typings": "index.d.ts",
  "module": "index.js",
  "scripts": {
    "build": "rimraf lib && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "jest": "jest",
    "lint": "npm run prettier && tslint -c tslint.json -p tsconfig.json -s '{src, test}/**/*.{ts}'",
    "prepublishOnly": "[[ \"$PWD\" =~ /lib ]]",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'",
    "pub": "pushd ./ && npm run build && cd lib && npm publish && popd",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/aol.git"
  },
  "keywords": [
    "Web mapping",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/jest": "^24.0.24",
    "@types/jszip": "^3.1.6",
    "@types/ol": "^5.3.6",
    "babel-jest": "^24.9.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "ol": "6.1.1",
    "prettier": "^1.19.1",
    "proj4": "2.6.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.7.3"
  },
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "bhreq": "0.0.12",
    "jszip": "^3.2.2",
    "ol-mapbox-style": "^5.0.2",
    "shapefile2geojson": "^1.0.7"
  },
  "peerDependencies": {
    "ol": "6.1.1",
    "proj4": "2.6.0"
  },
  "engines": {
    "node": ">= 10.10.0",
    "npm": ">= 6.4.1"
  }
}
