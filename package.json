{
  "name": "@gisosteam/aol",
  "version": "0.2.5",
  "description": "OpenLayers sources extensions",
  "scripts": {
    "test": "jest",
    "jest": "jest",
    "prettier": "prettier --single-quote --print-width 120 --write --parser typescript '{src, test}/**/*.ts'",
    "lint": "npm run prettier && eslint . --ext .ts,.tsx",
    "genindex": "barrelsby -d ./src -D -L -l all -q -e sample -e __tests__ -e .d.ts",
    "build": "rimraf lib && npm run genindex && npm run lint && tsc -p tsconfig.json && cp ./package.json lib",
    "prepublishOnly": "[[ \"$PWD\" =~ /lib ]]",
    "pub": "pushd ./ && npm run build && cd lib && npm publish --access public && popd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GisOsTeam/aol.git"
  },
  "keywords": [
    "Web mapping",
    "OpenLayers"
  ],
  "author": "Mathieu MAST",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@types/jest": "^29.0.0",
    "@types/jszip": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^29.0.2",
    "barrelsby": "^2.4.0",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "jest-canvas-mock": "^2.4.0",
    "ol": "^7.1.0",
    "prettier": "^2.7.1",
    "proj4": "2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "bhreq": "^0.1.2",
    "jszip": "^3.10.1",
    "ol-mapbox-style": "^9.2.0",
    "shapefile2geojson": "^1.0.7",
    "simplepolygon": "1.2.3"
  },
  "peerDependencies": {
    "@types/ol": "6.5.3",
    "ol": "^7.1.0",
    "proj4": "^2.8.0"
  },
  "engines": {
    "node": ">= 16.10.0",
    "npm": ">= 7.24.0"
  }
}
