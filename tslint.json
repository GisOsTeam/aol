{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "arrow-parens": false,
    "max-classes-per-file": false,
    "max-line-length": [
      false
    ],
    "no-console": [
      true,
      "log"
    ],
    "no-empty-interface": false,
    "object-literal-key-quotes": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      false,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "variable-name": [
      false
    ]
  }
}
